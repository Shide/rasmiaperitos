<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_form_mechanical_workshop" model="ir.ui.view">
        <field name="name">res.partner.form.mechanical.workshop</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='misc']" position="inside">
                <field name="labor_price"/>
                <field name="opening_schedule"/>
            </xpath>
        </field>
    </record>
</odoo>
